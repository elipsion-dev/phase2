    <div>
      <div class="container">
        <div class="row">
          <div class="col-md-8 offset-md-2">
            <div class="border-1px p-25">
              <h4 class="text-theme-colored1 text-uppercase m-0">Make an Appointment</h4>
              <div class="line-bottom mb-30"></div>
              <p>Schedule your appointment.</p>
              <div class="row">
                <div class="alert alert-danger d-none" role="alert" id="appointment_error_message">
                </div>
                <div class="col-md-6">
                  <div class="mb-3 mb-10">
                    <input id="appt_first_name" class="form-control" type="text" placeholder="Enter First Name*"
                      aria-required="true">
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3 mb-10">
                    <input id="appt_last_name" class="form-control" type="text" placeholder="Enter Last Name*"
                      aria-required="true">
                  </div>
                </div>
                <!-- test -->
                <!-- <input type="text" id="datetimepicker">    -->

                <div class="col-md-6">
                  <div class="mb-3 mb-10">
                    <input id="appt_email" class="form-control required email" type="email" placeholder="Enter Email*"
                      aria-required="true">
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3 mb-10">
                    <input id="appt_phone" class="form-control required" type="text" placeholder="Enter Phone*"
                      aria-required="true">
                  </div>
                </div>
                <div class="col-md-12">
                  <div class="mb-3 mb-10">
                    <select class="form-control" id="appt_doctor">
                      <option value="">Select Provider*</option>
                    </select>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3">
                    <input id="appt_appointment_date" disabled class="form-control" type="text">
                    <label for="appt_appointment_date">Appointment Date</label>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3">
                    <input id="appt_appointment_time" disabled class="form-control" type="text">
                    <label for="appt_appointment_time">Appointment Time</label>
                  </div>
                </div>

              </div>
              <div class="mb-3 mb-10">
                <textarea id="appt_appointment_message" class="form-control required" placeholder="Enter Message"
                  rows="5" aria-required="true"></textarea>
              </div>
              <div class="mb-3 tm-sc-button mb-0 mt-20">
                <input name="form_botcheck" class="form-control" type="hidden" value="">
                <button id="create_appointment" class="btn btn-theme-colored1 btn-sm">
                  <span id="create_appointment_text">Save Schedule</span>
                  <span id="create_appointment_text_spin" class="d-none">
                    <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                    Loading...
                  </span>
                </button>
              </div>

            </div>
          </div>
